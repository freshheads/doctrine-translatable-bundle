{
    "name": "prezent/doctrine-translatable-bundle",
    "type": "symfony-bundle",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "description": "Integrate the doctrine-translatable extension in Symfony2",
    "keywords": [
        "doctrine2",
        "extensions",
        "translatable",
        "i18n",
        "symfony2",
        "bundle"
    ],
    "homepage": "http://www.prezent.nl",
    "authors": [{
        "name": "Prezent Internet B.V.",
        "email": "github@prezent.nl"
    }],
    "license": "MIT",
    "support": {
        "wiki": "https://github.com/prezent/doctrine-translatable-bundle/tree/master/Resources/doc",
        "issues": "https://github.com/prezent/doctrine-translatable-bundle/issues",
        "source": "https://github.com/prezent/doctrine-translatable-bundle"
    },
    "require": {
        "doctrine/doctrine-bundle": "^1.10|^2.0",
        "prezent/doctrine-translatable": "^2.0|^3.0|^4.0",
        "symfony/config": "^6.4|^7.3",
        "symfony/dependency-injection": "^6.4|^7.3",
        "symfony/event-dispatcher": "^6.4|^7.3",
        "symfony/http-foundation": "^6.4|^7.3",
        "symfony/http-kernel": "^6.4|^7.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4|^7.3",
        "sonata-project/admin-bundle": "^4.0|^5.0",
        "sonata-project/doctrine-orm-admin-bundle": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": { "Prezent\\Doctrine\\TranslatableBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:freshheads/doctrine-translatable.git"
        }
    ]
}
